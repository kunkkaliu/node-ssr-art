{
  "name": "node_ssr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build_client": "webpack --config ./build/webpack.prod.config.js --progress --colors",
    "build_server": "webpack --config ./build/webpack.server.config.js --progress --colors",
    "build_dev": "rimraf ./dist && cross-env RUN_ENV=client CODE_ENV=dev npm run build_client && cross-env RUN_ENV=server CODE_ENV=dev npm run build_server",
    "build_qa": "rimraf ./dist && cross-env RUN_ENV=client CODE_ENV=qa npm run build_client && cross-env RUN_ENV=server CODE_ENV=qa npm run build_server",
    "build_online": "rimraf ./dist && cross-env RUN_ENV=client CODE_ENV=online npm run build_client && cross-env RUN_ENV=server CODE_ENV=online npm run build_server",
    "analyze": "cross-env ANALYZE=1 npm run build_client",
    "mock": "cross-env MOCK=1 npm run dev",
    "dev": "cross-env RUN_ENV=server CODE_ENV=dev node server/app.dev.js",
    "start": "cross-env node ./dist/server/index.js",
    "server": "pm2 start ./pm2config.json --env production --no-daemon",
    "online": "pm2 start ./pm2config.json --env production"
  },
  "author": "kunkkaliu",
  "license": "ISC",
